    "version": "0.2.0",
    "configurations": [
        {
            "name": "ApiLogicServer",
            "type": "python",
            "request": "launch",
            "program": "api_logic_server_run.py",
            "args": ["localhost", "5656"],
            "console": "integratedTerminal"
        },
        {
            "name": "Basic Web App",
            "type": "python",
            "request": "launch",
            "program": "run.py",
            "args": ["localhost", "5002"],
            "cwd": "${workspaceFolder}/ui/basic_web_app",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            "module": "enter-your-module-name"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}